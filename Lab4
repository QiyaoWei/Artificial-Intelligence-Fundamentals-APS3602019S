      "source": [
        "#lazy way to only take one component out\n",
        "for i in ts_loader:\n",
        "  item = i[0]\n",
        "  inp = item.detach().numpy()\n",
        "  datam = ae(zero_out_feature(item.clone(), 'edu')).detach().numpy()\n",
        "  temp = cat_index['edu']\n",
        "  print(cat_values['edu'][np.argmax(datam[0][temp:temp+len(cat_values['edu'])])])\n",
        "  break\n",
        "\n",
        "#My model is accurate"
