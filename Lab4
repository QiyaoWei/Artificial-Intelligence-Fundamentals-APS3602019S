        "for i in ts_loader:\n",
        "  temp = get_onehot(ts_data, 'edu')\n",
        "  index = np.argmax(temp.sum(axis=1))\n",
        "  item = i[0]\n",
        "  inp = item.detach().numpy()\n",
        "  out = zero_out_feature(item.clone(), 'edu').detach().numpy()\n",
        "  out[:, index + cat_index['edu']] = 1\n",
        "  print(cat_values['edu'][index])\n",
        "  break\n",
        "\n",
