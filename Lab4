        "import matplotlib.pyplot as plt\n",
        "ae = AutoEncoder()\n",
        "tr_acc, vl_acc, tr_loss = train(ae, tr_loader, vl_loader, num_epochs=100)\n",
        "\n",
        "plt.subplot(1, 2, 1)\n",
        "l = range(len(tr_acc))\n",
        "plt.ylabel('Accuracy')\n",
        "plt.xlabel('Epochs')\n",
        "plt.title(\"Accuracy vs. Epochs\")\n",
        "plt.tight_layout()\n",
        "plt.plot(l, tr_acc, label = \"Training Accuracy\", color = \"red\")\n",
        "plt.plot(l, vl_acc, label = \"Validation Accuracy\", color = \"green\")\n",
        "plt.legend()\n",
