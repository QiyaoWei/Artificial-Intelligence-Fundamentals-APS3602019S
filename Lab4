        "#It doesn't seem necessary to make age values into an integer for this purpose\n",
        "print(\"max age = \", np.max(df['age']))\n",
        "print(\"min age = \", np.min(df['age']))\n",
        "print(\"avg age = \", np.sum(df['age'])/len(df['age']))\n",
        "df['age'] = df['age'] / np.max(df['age'])\n",
        "print(\"max yredu = \", np.max(df['yredu']))\n",
        "print(\"min yredu = \", np.min(df['yredu']))\n",
        "print(\"avg yredu = \", np.sum(df['yredu'])/len(df['yredu']))\n",
        "df['yredu'] = df['yredu'] / np.max(df['yredu'])\n",
        "print(\"max capgain = \", np.max(df['capgain']))\n",
